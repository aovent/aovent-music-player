{
  "name": "music-player",
  "version": "2.1.0",
  "description": "Aovent Music Player",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fs": "^0.0.1-security"
  },
  "build": {
    "appId": "aovent.music.player.0002",
    "productName": "Aovent MP",
    "win": {
      "icon": "./src/img/aovent.png",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./src/img/aovent.png",
      "license": "./license.txt"
    },
    "mac": {
      "icon": "./src/img/aovent.png"
    },
    "linux": {
      "icon": "./src/img/aovent.png"
    }
  },
  "devDependencies": {
    "electron-builder": "^23.6.0",
    "electron": "^24.3.0"
  }
}
